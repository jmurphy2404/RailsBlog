<div class="postscontain">
  <div class="allpostsimg">
    <img src="https://20843-presscdn-pagely.netdna-ssl.com/wp-content/themes/pf-youngadventuress-2016/img/map.jpg" />
  </div>
  <h1 class="allpostsheader">All Users</h1>
</div>

<div class="usertable">
<table>
    <% @users.each do |user| %>
      <tr>
        <td class="userimage"><%= image_tag(user.image) if user.image? %></td>
        <td class="useruser"><%= link_to user.fullname, user %></td>
      </tr>
    <% end %>
</table>
</div>